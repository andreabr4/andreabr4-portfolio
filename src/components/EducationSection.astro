---
import EducationItem from "./EducationItem.astro";
import University from "../components/icons/University.astro";

const education = [ 
  {
    description: "Lorem ipsum dolor sit",
  },
];
---

<ol class="relative border-s border-gray-200 dark:border-gray-700 ml-3">
  {
    education.map((course) => (
      <li>
        <University />,
        <EducationItem {...course} />
      </li>
    ))
  }
</ol>
